        target: /src
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Helios\vsdbg\vs2017u5
        target: /remote_debugger
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Helios\.nuget\packages
